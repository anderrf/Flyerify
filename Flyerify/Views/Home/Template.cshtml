@{

    ViewData["Title"] = "Template";
}
@model TemplateModel;
<div class="w-100 template-focus-display">
    <img src="@Model.imagePath" alt="">
    <div class="template-info">
        <div class="template-title">
            <h3>@Model.title</h3>
            <p>@Model.theme</p>
        </div>
        <p class="template-description">@Model.description</p>
    </div>
</div>